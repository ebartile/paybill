{
  "name": "@paybilldev/sequelize",
  "version": "0.0.1-beta.1",
  "description": "A Sequelize integration package for PaybillDev, providing database models, utilities, and ORM configurations for easy setup and management.",
  "license": "AGPL-3.0-only",
  "type": "commonjs",
  "source": "src/index.tsx",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "sideEffects": false,
  "keywords": [
    "sequelize",
    "orm",
    "database",
    "models"
  ],
  "author": {
    "name": "ebartile",
    "email": "ebartile@gmail.com"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "tsup",
    "postbuild": "attw --pack",
    "clean": "rimraf -g {node_modules,dist,*.tsbuildinfo,.turbo,coverage,log}",
    "typecheck": "tsc --project tsconfig.json",
    "test": "NODE_ENV=test ENCRYPTION_FIELD_KEY='Xj8P1!zB@7qR$Lm^2&vWc#nF0*Hs9YpD' vitest",
    "cov": "NODE_ENV=test ENCRYPTION_FIELD_KEY='Xj8P1!zB@7qR$Lm^2&vWc#nF0*Hs9YpD' vitest --coverage",
    "prepublishOnly": "pnpm build"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paybilldev/paybill.git",
    "directory": "packages/sequelize"
  },
  "dependencies": {
    "@rc-component/mini-decimal": "^1.1.0",
    "async-mutex": "^0.5.0",
    "chalk": "^4.1.2",
    "dayjs": "1.11.13",
    "debug": "4.4.1",
    "deepmerge": "^4.3.1",
    "dottie": "2.0.6",
    "excel-date-to-js": "^1.1.5",
    "exponential-backoff": "^3.1.2",
    "flat": "^5.0.2",
    "glob": "11.0.0",
    "lodash": "^4.17.21",
    "nanoid": "^3.3.11",
    "node-sql-parser": "^4.18.0",
    "qs": "^6.14.0",
    "rimraf": "^6.0.0",
    "safe-json-stringify": "^1.2.0",
    "semver": "7.7.2",
    "sequelize": "^6.37.7",
    "umzug": "^3.8.2",
    "unbuild": "catalog:"
  },
  "peerDependencies": {
    "mssql": "^11.0.1",
    "mysql": "^2.18.1",
    "mysql2": "^3.15.0",
    "oracledb": "^6.9.0",
    "pg": "^8.16.3",
    "sqlite3": "^5.1.7"
  },
  "peerDependenciesMeta": {
    "mssql": {
      "optional": true
    },
    "mysql": {
      "optional": true
    },
    "mysql2": {
      "optional": true
    },
    "oracledb": {
      "optional": true
    },
    "pg": {
      "optional": true
    },
    "sqlite3": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/flat": "^5.0.5",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.5.2",
    "@types/qs": "^6.14.0",
    "@types/semver": "7.7.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@arethetypeswrong/cli": "^0.18.2",
    "rimraf": "6",
    "tsup": "^8.5.0",
    "ts-node": "10.9.2",
    "typescript": "catalog:",
    "vitest": "^3.0.5"
  }
}
