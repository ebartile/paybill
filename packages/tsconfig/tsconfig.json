{
	"compilerOptions": {
		"module": "NodeNext",
		"target": "ESNext",
		"lib": ["es6", "es7", "esnext"],
		"types": ["node"],
		"moduleResolution": "nodenext",
		"noImplicitReturns": true,
		"strict": true,
		"strictFunctionTypes": true,
		"noUnusedLocals": true,
		"pretty": true,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"downlevelIteration": true,
		"skipLibCheck": true,
		"listEmittedFiles": true,
		"noEmit": true
	}
}
