{
  "name": "@paybilldev/acl",
  "version": "0.0.1-beta.1",
  "description": "Access Control List (ACL) package for PaybillDev, providing role-based permissions and authorization utilities for managing user access.",
  "license": "AGPL-3.0-only",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "keywords": [
    "acl"
  ],
  "author": {
    "name": "ebartile",
    "email": "ebartile@gmail.com"
  },
  "scripts": {
    "build": "unbuild --clean",
    "stub": "unbuild --stub",
    "clean": "rimraf node_modules dist .turbo coverage",
    "typecheck": "tsc --project tsconfig.json",
    "test": "NODE_ENV=test vitest",
    "cov": "NODE_ENV=test vitest --coverage",
    "prepublishOnly": "pnpm build"
  },
  "engines": {
    "node": ">=20"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/paybilldev/paybill.git",
    "directory": "packages/acl"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "dayjs": "1.11.13",
    "lodash": "4.17.21",
    "minimatch": "^10.0.3"
  },
  "devDependencies": {
    "@paybilldev/tsconfig": "workspace:^",
    "@types/lodash": "4.17.20",
    "@types/node": "20.17.10",
    "@vitest/coverage-istanbul": "^3.2.4",
    "rimraf": "6",
    "ts-node": "10.9.2",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vitest": "^3.0.5"
  }
}
